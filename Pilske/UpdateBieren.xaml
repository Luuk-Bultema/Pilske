<Window x:Class="Pilske.UpdateBieren"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pilske"
        mc:Ignorable="d"
        Title="UpdateBieren" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtBier" HorizontalAlignment="Left" Height="23" Margin="110,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.082,-0.009"/>
        <TextBlock HorizontalAlignment="Left" Margin="45,47,0,0" TextWrapping="Wrap" Text="Bier" VerticalAlignment="Top"/>
        <Button x:Name="BtnUpdateB" Content="Update Bier
                " HorizontalAlignment="Left" Margin="165,95,0,0" VerticalAlignment="Top" Width="85" Click="BtnUpdateB_Click" Height="20"/>

    </Grid>
</Window>

<Window x:Class="Pilske.UpdateHapjes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Pilske"
        mc:Ignorable="d"
        Title="UpdateHapje" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="txtHapje" HorizontalAlignment="Left" Height="23" Margin="110,40,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" RenderTransformOrigin="0.082,-0.009"/>
        <TextBlock HorizontalAlignment="Left" Margin="45,47,0,0" TextWrapping="Wrap" Text="Hapje" VerticalAlignment="Top" Width="35" Height="33"/>
        <Button x:Name="BtnUpdate" Content="Update Hapjes" HorizontalAlignment="Left" Margin="155,96,0,0" VerticalAlignment="Top" Width="105" Height="24" Click="BtnUpdate_Click"/>

    </Grid>
</Window>
